   db:refpurpose [ "" ]
]
div {

   local.type.attributes = empty

   type.role.attribute = attribute role { text }

   type.attlist =
      type.role.attribute?
    & common.attributes
    & common.linking.attributes
    & local.type.attributes

   db.type =
      element type {
         type.attlist,
         docbook.text
      }
}

